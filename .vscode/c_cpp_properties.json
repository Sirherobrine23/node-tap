{
	"version": 4,
	"configurations": [
		{
			"name": "Win32",
			"intelliSenseMode": "windows-msvc-x64",
			"includePath": [
				"${workspaceFolder}/node_modules/node-addon-api/**",
				"${workspaceFolder}/node_modules/node-api-headers/include/**",
				"${workspaceFolder}/src/TAP/**"
			],
			"defines": [
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			]
		},
		{
			"name": "Linux",
			"includePath": [
				"${workspaceFolder}/node_modules/node-addon-api/**",
				"${workspaceFolder}/node_modules/node-api-headers/include/**",
				"${workspaceFolder}/src/TAP/**"
			],
			"defines": [],
			"compilerPath": "/usr/bin/clang",
			"cStandard": "c99",
			"cppStandard": "c++20",
			"intelliSenseMode": "linux-clang-x64",
			"compilerArgs": [
				"-I${workspaceFolder}/node_modules/node-addon-api",
				"-fdiagnostics-color=always",
				"-fno-rtti",
				"-fno-exceptions",
				"-std=c++20",
				"-fpermissive",
				"-fno-exceptions",
				"-w",
				"-fpermissive",
				"-fPIC",
				"-static"
			]
		}
	]
}